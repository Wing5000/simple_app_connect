# Serwer
PORT=8080
NODE_ENV=development
API_KEY=supersecretapikey

# Baza (dev: SQLite, prod: Postgres)
DATABASE_URL="file:./dev.db"

# Redis do BullMQ
REDIS_URL=redis://localhost:6379

# Starknet
STARKNET_NETWORK=sepolia
STARKNET_RPC_URL=https://rpc.starknet.sepolia.example
# Jedno z poni≈ºszych (dev: klucz lokalny; prod: KMS)
STARKNET_ACCOUNT_ADDRESS=0xYOUR_ACCOUNT
STARKNET_PRIVATE_KEY=0xYOUR_PRIVATE_KEY
# KMS (opcjonalnie)
KMS_PROVIDER=aws
KMS_KEY_ID=your-kms-key-id

# Opcjonalne szyfrowanie odpowiedzi (RSA public key w PEM)
RESPONSE_ENCRYPTION_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----"
